 #include <stdio.h>
int main() {
  int a,b,c=1;
  for(a=0;a<=4;a++)
  {
    for(b=1;b<=a;b++)
    {
        printf("%d\t",c++);
    }
    printf("\n");
  }
}
[ Three Triangle Pattern]


 #include<stdio.h>

int main(){
    int a,b,sum,min,div,mul,choice;
    printf("1. for sum\n2. for min\n3. for mul\n4. for div\n enter your choicse : ");
    scanf("%d",&choice);
    printf("enter number 1 : ");
    scanf("%d",&a);
    printf("enter number 2 : ");
    scanf("%d",&b);
    switch(choice)
    {
        case 1:
            printf("%d",a+b);
            break;
        case 2:
            printf("%d",a-b);
            break;
        case 3:
            printf("%d",a*b);
            break;
        case 4: 
            printf("%d",a/b);
            break;
        default:
            printf("invalid output!");
            break;
    }
}
[Switch case for aethmetic operation]


#include<stdio.h>

int main(){
    int a;
    printf("enter number from 1 to 5 : ");
    scanf("%d",&a);
    switch(a)
    {
        case 1:
            printf("ONE");
            break;
        case 2:
            printf("TWO");
            break;
         case 3:
            printf("THREE");
            break;
         case 4:
            printf("FOUR");
            break;
         case 5:
            printf("FIVE");
            break;
         default:
            printf("invalid");
    }
}
