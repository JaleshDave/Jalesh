#include <stdio.h>
#include <stdio.h>
int main()
{
    int i,j,k,l=1;

    for (i = 0; i < 5; i++) {

        for (j = 0; j < (5 - i) - 1; j++) {
            printf(" ");
        }

        for (k = 1; k <= i + 1; k++) {
            printf("%d ",k);
        }
        printf("\n");
    }
}
[Fifth triangle pattern]


 #include <stdio.h>
int main() {
  int t1 = 0, t2 = 1, nt = 0, n;
  printf("Enter a positive number: ");
  scanf("%d", &n);

  printf("Fibonacci Series: %d, %d, ", t1, t2);
  nt = t1 + t2;

  while (nt <= n) {
    printf("%d, ", nt);
    t1 = t2;
    t2 = nt;
    nt = t1 + t2;
  }
  return 0;
}
