[ Positive negative]


 #include<stdio.h>

int main()
{
   int num, temp, rem, rev = 0;
   printf("enter a number:");
   scanf("%d", &num);
   temp = num;
   while ( temp > 0){
      rem = temp %10;
      rev = rev *10+ rem;
      temp = temp /10;
   }
   printf("reversed number is = %d", rev);
   if ( num == rev )
   {
      printf("%d is Palindrome Number.", num);
   }
   return 0;
}
[10/5, 13:24] jaleshdave: Palindrome
