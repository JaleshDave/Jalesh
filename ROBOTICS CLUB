    scanf("%d",&x);
    printf("enter power : ");
    scanf("%d",&y);
    ans = pow(x, y);
    printf("%d", ans);   
}
[Finding Power]


#include<stdio.h>
int main()
{
    int a,b,result=1;
    printf("enter base : ");
    scanf("%d",&a);
    printf("enter exponent : ");
    scanf("%d",&b);
    while(b!=0)
    {
        result *= a;
        --b;
    }
    printf("%d",result);
}
