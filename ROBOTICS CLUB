#include <stdio.h>
#include <math.h>

int main() {
    int c,d,ds,dm,avg;
    printf("enter marks in c language : ");
    scanf("%d",&c);
    printf("enter marks in dbms : ");
    scanf("%d",&d);
    printf("enter marks in dsa : ");
    scanf("%d",&ds);
    printf("enter marks in dm : ");
    scanf("%d",&dm);
    avg=(c+ds+dm+d)/4;
    if(avg>90)
    {
        printf("A");
    }
    else if(avg>80){
        printf("B+");
    }
    else if(avg>70){
        printf("B");
    }
    else if(avg>60){
        printf("C+");
    }
    else if(avg>50){
        printf("C");
    }
    else if(avg>40){
        printf("D");
    }
    else{
        printf("fail");
    }
}
[ Marks average]


#include <stdio.h>
#include <stdio.h>

int main()
{
    int i,j,k;

    for (i = 0; i < 5; i++) {

        for (j = 0; j < (5 - i) - 1; j++) {
            printf(" ");
        }

        for (k = 0; k < i + 1; k++) {
            printf("* ");
        }
        printf("\n");
    }
}
