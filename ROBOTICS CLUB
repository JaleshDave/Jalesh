[Input output of Arrays]


 #include<stdio.h>
int main(){
    int l,b,s,r,ba,he,hy,ra,rp,sa,sp,ca,cc,ta,tp;
    printf("***************************FOR RECTANGLE***********************************");
    printf("enter length of rectangle : ");
    scanf("%d",&l);
    printf("enter width of rectangle : ");
    scanf("%d",&b);
    ra=l*b;
    rp=2*(l+b);
    printf("area of rectangle is %d\nperimeter of rectangle is %d",ra,rp);
    printf("***************************FOR SQUARE***********************************");
    printf("enter side of square: ");
    scanf("%d",&s);
    sa=s*s;
    sp=4*s;
    printf("area of square is %d\nperimeter of square is %d",sa,sp);
    printf("***************************FOR CIRCLE***********************************");
    printf("enter radius: ");
    scanf("%d",&r);
    ca=3.14*(r*r);
    cc=(2*(3.14)*r);
    printf("area of circle is %d\nperimeter of circle is %d",ca,cc);
    printf("***************************FOR triangle***********************************");
    printf("enter base of triangle : ");
    scanf("%d",&ba);
    printf("enter height of triangle : ");
    scanf("%d",&he);
    printf("enter hypotenuse of triangle : ");
    printf("%d",&hy);
    ta=0.5*(ba*he);
    tp=ba+he+hy;
    printf("area of triangle is %d",ta);
    printf("perimeter of triagnle is %d",tp);
}
[Area perimeter of 2D Shapes]


#include <stdio.h>
int main()
{
  int a[5][5],i,j;
  for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("enter the elements at a[%d][%d]: ",i,j);
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d\t",a[i][j]);
        }
        printf("\n");
    }
}
[ 2d printing and scanning]


 #include<stdio.h>
int main(){
    int a;
    for(a=1;a<=10;a++)
    {
        printf("%d\n",a);
    }
    printf("REVERSED\n");
    for(a=10;a>0;a--)
    {
        printf("%d\n",a);
    }
}
[ Reverse number]